        "fmt/validator": "^1.0",
        "fmt/consola": "^1.1",
        "fmt/usuarios": "^1.10",
        "fmt/ubicaciones": "^1.0",
        "dompdf/dompdf": "^0.8.2",
        "fmt/informacion_fecha" : "^2",
